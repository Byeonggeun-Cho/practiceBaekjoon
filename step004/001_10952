// while 반복구문
// if, break 이요한 조건부 탈출

import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int a = 1;
        int b = 1;
        
        while(true){
            a = sc.nextInt();
            b = sc.nextInt();
            
            if(a == 0 && b == 0){
                break;
            } else{
                System.out.println(a+b);
            }
        }
    }
}
