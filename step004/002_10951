// 입력 값에 따른 반복 탈출
// 참조: https://mygumi.tistory.com/236

import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        int a = 1;
        int b = 1;
        
        while(sc.hasNext()){
            a = sc.nextInt();
            b = sc.nextInt();
            
            System.out.println(a+b);
        }
    }
}
