import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);

        int a = 0;
        int b = 0;
        
        int n = sc.nextInt();
        
        for(int i=1; i<=n; i++){
            
            a = sc.nextInt();
            b = sc.nextInt();

            System.out.println("Case #" + i + ": " + (a+b));
        }
    }
}

