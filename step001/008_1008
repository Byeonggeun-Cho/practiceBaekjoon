// import
// Scanner를 이용한 입력
// 자료형 double

import java.util.Scanner;

public class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        
        double a = sc.nextInt();
        double b = sc.nextInt();
        
        if(b != 0){
            System.out.println(a / b);
        }
    }
}
